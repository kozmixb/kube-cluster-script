{
  "metadata": {
    "name": "ingress-nginx-custom",
    "annotations": {
      "kubernetes.io/ingress.class": "nginx"
    }
  },
  "spec": {
    "rules": [
      {
        "host": "*.bence.local",
        "http": {
          "paths": [
            {
              "path": "/",
              "backend": {
                "serviceName": "nginx-custom",
                "servicePort": 80
              }
            }
          ]
        }
      }
    ]
  }
}