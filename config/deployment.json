{
  "metadata": {
    "name": "test"
  },
  "spec": {
    "replicas": 1,
    "strategy": {
      "type": "RollingUpdate"
    },
    "selector": {
      "matchLabels": {
        "app": "web"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "web"
        }
      },
      "spec": {
        "containers": [],
        "volumes":[
          {
            "name": "nginx-config",
            "configMap": {
              "name": "confnginx"
            }
          }
        ]
      }
    }
  }
}